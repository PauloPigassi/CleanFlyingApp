<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d3d7cf"
    tools:context="br.com.onder.cleanflyingos.CadastroServicoActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical|center_horizontal|center"
                android:layout_weight="1.1"
                android:gravity="center_vertical|center_horizontal|center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/airplanef" />

                <TextView
                    android:id="@+id/tv_serv_prefixo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:gravity="center_vertical|center_horizontal"
                    android:paddingLeft="10dp"
                    android:text="TextView"
                    android:textAlignment="viewStart"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    tools:ignore="RtlCompat" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="/"
                    android:textAlignment="center"
                    android:textColor="#CCCCCC"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tv_serv_modelo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="TextView"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    android:textStyle="bold" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="left"
                android:layout_weight="1.1"
                android:gravity="left|center_horizontal"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.3"
                    android:scaleType="fitStart"
                    app:srcCompat="@drawable/building" />

                <TextView
                    android:id="@+id/tv_serv_cliente"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="TextView"
                    android:textColor="#000000"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1"
                    android:text="/"
                    android:textAlignment="center"
                    android:textColor="#CCCCCC" />

                <TextView
                    android:id="@+id/tv_serv_setor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="TextView"
                    android:textColor="#000000"
                    android:textStyle="bold" />
            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_serv_serv"
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:background="#303638"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center_horizontal|center"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="SERV"
                    android:textColor="#ffffff"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/ll_serv_ckum"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:background="#888a85"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal|center"
                    android:text="CKL1"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="30dp"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/ll_serv_ckdois"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:background="#888a85"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal|center"
                    android:text="CKL2"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="30dp"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/ll_serv_obs"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:background="#888a85"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal|center"
                    android:text="OBS"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="30dp"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:id="@+id/ll_serv_resp"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:background="#888a85"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical|center_horizontal|center"
                    android:text="RESP"
                    android:textSize="18sp"
                    android:textStyle="bold" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#303638"
            android:orientation="horizontal"
            android:padding="8dp">

            <LinearLayout
                android:layout_width="10dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/ck_serv_atendimento"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Atendimento" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"></LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/ck_serv_manutencao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Manutenção" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="8dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"></LinearLayout>

            <LinearLayout
                android:layout_width="88dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/ck_serv_avulso"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Avulso" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="01 - Higienização Interrna com Aspiração"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_1"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_2"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="02 - Higienização Interna Completa"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_2"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_3"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="03 - Lavagem Externa"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_3"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_4"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="04 - Lavagem Externa á Seco"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_4"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_5"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="05 - Lavagem para Lubrificação"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_5"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_6"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="06 - Polimento Completo"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_6"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_7"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="07 - Polimento de Partes Metálicas"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_7"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_8"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="08 - Higienização de Carpetes"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_8"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_9"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="09 - Higienizaçao de Estofados"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_9"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_10"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="10 - Higienização de QTU/QTA"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_10"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#babbbc"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_11"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="11 - Limpeza de Para-brisas/Bolhas"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_11"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:background="#aaaaaa"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ck_serv_12"
                android:layout_width="288dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:buttonTint="#000000"
                android:text="12 - Outros"
                android:textColor="#000000"
                android:textColorLink="#000000" />

            <EditText
                android:id="@+id/et_obs_12"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:hint="OBS"
                android:inputType="textPersonName"
                android:textColorLink="#CCCCCC"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#000000"
            android:orientation="horizontal"></LinearLayout>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>

